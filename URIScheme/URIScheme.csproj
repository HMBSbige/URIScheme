<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net5.0-windows;netcoreapp3.1;netstandard2.0</TargetFrameworks>
		<LangVersion>latest</LangVersion>
		<Nullable>enable</Nullable>
		<Copyright>Copyright Â© HMBSbige 2020</Copyright>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Authors>HMBSbige</Authors>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageProjectUrl>https://github.com/HMBSbige/URIScheme</PackageProjectUrl>
		<RepositoryUrl>https://github.com/HMBSbige/URIScheme</RepositoryUrl>
		<PackageTags>Windows URI Scheme</PackageTags>
		<Version>5.0.0</Version>
		<PackageId>URIScheme</PackageId>
		<Description>Registering an Application to a URI Scheme.</Description>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Win32.Registry" Version="5.0.0" />
	</ItemGroup>

</Project>